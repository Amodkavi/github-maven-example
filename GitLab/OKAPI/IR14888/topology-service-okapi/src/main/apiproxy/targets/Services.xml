<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<TargetEndpoint name="Services">
    <Description/>
    <FaultRules/>
    <PreFlow name="PreFlow">
        <Request>
            <Step>
                <Name>Get-CRAMER-LI-Basic-Auth-User-Id</Name>
            </Step>
            <Step>
                <Name>Get-CRAMER-LI-Basic-Auth-Password</Name>
            </Step>
            <Step>
                <Name>ApplyBasicAuthHeader</Name>
            </Step>
        </Request>
        <Response/>
    </PreFlow>
    <PostFlow name="PostFlow">
        <Request/>
        <Response/>
    </PostFlow>
    <Flows/>
    <HTTPTargetConnection>
        <Properties>
            <Property name="success.codes">200,202,400,404,500</Property>
        </Properties>
        <LoadBalancer>
            <Server name="topology-service-cramerli"/>
        </LoadBalancer>
        <Path>/invapi/v1</Path>
    </HTTPTargetConnection>
</TargetEndpoint>